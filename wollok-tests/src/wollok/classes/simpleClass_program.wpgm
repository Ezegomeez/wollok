import simpleClass.Golondrina

program simpleClass {
	
	const pepita = new Golondrina()

	assert.equals(100, pepita.getEnergia())
	
	//// una segunda
	//
	const pepona = new Golondrina()
	pepona.volar(2)
	
	assert.equals(98, pepona.getEnergia())
	assert.equals(100, pepita.getEnergia())  // pepita no se altero
	//
	//

}