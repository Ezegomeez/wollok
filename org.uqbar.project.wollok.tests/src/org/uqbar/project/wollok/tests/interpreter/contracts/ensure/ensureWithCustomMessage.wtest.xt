/* 
   XPECT_SETUP org.uqbar.project.wollok.tests.interpreter.WollokXPectInterpreterTestCase END_SETUP
   XPECT interpret 
*/

class Bird {
	var currentCity
	
	method flyTo(city) {
		currentCity = city
	}
	ensures city != null : "A bird got out of the world"
}

test "simplePreConditionForSingleParameterInClass" {
	const b = new Bird()
	assert.throwsExceptionWithMessage("Method post conditions not met: A bird got out of the world (city != null)", { => b.flyTo(null) })
}