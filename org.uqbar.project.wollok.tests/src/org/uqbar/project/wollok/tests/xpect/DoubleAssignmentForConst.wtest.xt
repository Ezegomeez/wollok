/* XPECT_SETUP org.uqbar.project.wollok.tests.xpect.WollokXPectTest END_SETUP */
class X {
	const e
	constructor(_e) {
		// XPECT errors --> "Cannot modify constants" at "e"
		e = _e
		// XPECT errors --> "Cannot modify constants" at "e"
		e = 2
	}
}

class Y {
	const e = 1
	constructor(_e) {
		e = _e
	}
}

describe "group of tests" {
	const a
	
	fixture {
		// XPECT errors --> "Cannot modify constants" at "a"
		a = new X(10)
		// XPECT errors --> "Cannot modify constants" at "a"
		a = 8
	}
	
	test "easy test" {
		assert.equals(8, a)
	}
	
}