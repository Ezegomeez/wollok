/* XPECT_SETUP org.uqbar.project.wollok.tests.typesystem.xpect.TypeSystemXpectTestCase END_SETUP */

class Golondrina { 
	var energia = 100
	
	method vola() { energia = energia - 10 }
}

object pepita {
	method nombreAve() = "Pepita"
}

class Entrenador {
	method entrena(ave) { 
		ave.vola()
		// XPECT type at ave --> Golondrina
		ave
	}
	
	method alimentar(ave) {
		ave.nombreAve()
		// XPECT type at ave --> pepita
		ave
	}
}